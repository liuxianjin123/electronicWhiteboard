.container {
    width: 1920px;
    height: 1080px;
    user-select: none;
    background: #fff;
}

.container-top {
    height: 1020px;
    display: flex;
    justify-content: center;
    border-radius: 40px;
    background-size: cover !important;
}

ul li {
    list-style: none;
    color: #fff;
    transition: all .3s;
}

.container-item-left,
.container-item-right {
    height: 100%;
}

.container-top .container-item-left {
    width: 120px;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    padding: 20px 0 0 7px;
}

.container-top .container-item-right {
    width: 1805px;
}

.container-bottom {
    height: 60px;
}

.container-item-left li {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 33px;
    cursor: pointer;
    margin-top: 30px;
}

.container-item-left li img {
    width: 30px;
    height: 30px;
    margin-left: 3px;
}

.liCkeced {
    color: #4D54D8;
}


/*医院名称表头*/

.container-item-right header {
    height: 150px;
}

.container-item-right header .title,
.container-item-right header nav {
    margin: auto;
    width: 1793px;
    height: 90px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 17px;
    font-weight: 600;
}

.container-item-right header .title .title-icon {
    display: flex;
    align-items: center;
}

.container-item-right header .title .title-icon img {
    height: 70px;
    width: 70px;
    margin-right: 20px;
}

.container-item-right header .title .title-icon h1,
.title-date {
    font-weight: bold;
    color: #196D9D;
    margin: 0;
}

.container-item-right header .title .title-date {
    font-size: 21px;
    margin-right: 20px;
}

.container-item-right header nav {
    width: 1793px;
    height: 60px;
    border-radius: 50px;
    background: #fff;
    padding: 0 20px;
}


/*中间配置区域*/

.container-item-right section {
    height: 870px;
    width: 1793px;
    margin: 0 auto;
    position: relative;
    overflow: hidden;
}

.container-item-right section>table {
    height: 870px;
}

.container-bottom {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    padding: 0 20px;
}

.container-bottom i {
    font-size: 40px;
    color: #196D9D;
    margin: 0 20px;
    cursor: pointer;
}

.shadow {
    box-shadow: 3px 0px 7px 7px #dedddd;
}


/*各个容器*/

.itemBox {
    border-radius: 30px;
    background: #fff;
    margin: 20px 10px;
    padding: 15px;
}

.item-config table {
    height: 870px;
    width: 100%
}

.item-config table td {
    box-sizing: border-box;
    vertical-align: top;
}

.item-config table td:nth-child(-n+3) {
    width: 520px
}

.item-config table td:last-child {
    width: 230px
}

.sleectFz {
    border: 1px solid #1e9fff;
}

.item-box-children-title {
    display: flex;
    align-items: center;
}

.item-box-children-title i {
    display: inline-block;
    margin-right: 5px;
    border-radius: 3px;
}

.item-box-children-title h2 {
    flex: 1;
    margin: 0px;
    font-size: 20px;
    font-weight: bold;
}

.alerts {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 40px;
    border: 1px solid #91d5ff;
    border-radius: 5px;
    background: #e6f7ff;
    padding: 0 13px;
}

.texts {
    width: 100%;
    min-height: 28px;
    padding-left: 21px;
    font-size: 22px;
    /* overflow: hidden;
    text-overflow: ellipsis; */
}

.texts span {
    align-items: center;
    font-weight: bold;
    color:#196D9D;
}

.shadow>>>.ant-badge-status-dot {
    width: 13px;
    height: 13px;
}

.swiper-slide {
    word-break: break-all;
}

.spin-content {
    border: 1px solid #91d5ff;
    background-color: #e6f7ff;
    padding: 30px;
}

.item-config>>>.ant-spin-nested-loading {
    height: 100%;
}

.item-config>>>.ant-spin-nested-loading>div>.ant-spin {
    max-height: inherit !important;
}
.shade{
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 1;
}