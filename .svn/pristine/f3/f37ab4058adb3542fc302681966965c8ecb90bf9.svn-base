              title:'当前病区总患者 ',
              childrenSelectSql:'',
              ChildrenSelectParameters:'',
              childrenValue: ""
            },
            { 
              Id:"jrry",
              title:'今日入院人数',
              childrenSelectSql:'',
              ChildrenSelectParameters:'',
              childrenValue: ""
            },
            { 
              Id:"jrcy",
              title:'今日出院人数 ',
              childrenSelectSql:'',
              ChildrenSelectParameters:'',
              childrenValue: ""
            },
            { 
              Id:"tjhl",
              title:'特级护理患者数 ',
              childrenSelectSql:'',
              ChildrenSelectParameters:'',
              childrenValue: ""
            },
            { 
              Id:"yjhl",
              title:'一级护理患者数 ',
