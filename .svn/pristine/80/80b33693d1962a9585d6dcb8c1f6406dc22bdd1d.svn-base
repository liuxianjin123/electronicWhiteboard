var obj = {
    prop: {
        isEdit: Boolean,
        editRowData: Object,
    },
    watch: {
        isEdit(n, o) {
            this.isEdit = n;
            if (!this.isEdit) {
                for (var i in this.form) {
                    if (i != "type" && i != "title") {
                        this.form[i] = "";
                    }
                }
            }
        },
        editRowData(n, o) {
            let obj =  JSON.parse(JSON.stringify(n));
            for(var i in obj){
                console.log(i);
                for(var y in this.form){
                    console.log(y)
                    if(y==i){
                        this.form[y]=obj[i];
                    }
                }
            }
        }
    }
}

export default obj